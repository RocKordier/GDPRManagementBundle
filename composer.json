{
    "name": "ehdev/gdpr-management-bundle",
    "description": "GDPR Manager for OroPlatform",
    "homepage": "https://github.com/RocKordier/GDPRManagementBundle",
    "keywords": ["Oro Platform", "Symfony2", "GDPR"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "[RocKordier] Eric Hertwig",
            "homepage": "http://eric-hertwig.de"
        },
        {
            "name": "[Baachi] Markus Bachmann"
        }
    ],
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "autoload": {
        "psr-4": {
            "EHDev\\GDPRManagementBundle\\": ["src/", "tests/"]
        }
    },
    "require": {
        "php": ">=7.1",
        "oro/platform": "~2.6",
        "ehdev/oro-basics-bundle": "^0.0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "ehdev/ciutility": "^0.0.12"
    },
    "config" : {
        "fxp-asset" : {
            "enabled" : false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
